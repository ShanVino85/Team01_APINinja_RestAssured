{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "patientId": {
      "type": "integer"
    },
    "FirstName": {
      "type": "string"
    },
    "LastName": {
      "type": "string"
    },
    "ContactNumber": {
      "type": "string"
    },
    "Email": {
      "type": "string"
    },
    "Allergy": {
      "type": "string"
    },
    "FoodPreference": {
      "type": "string"
    },
    "CuisineCategory": {
      "type": "string"
    },
    "FileMorbidity": {
      "type": "object",
      "properties": {
        "^[0-9]{10}$": {
          "type": "object",
          "properties": {
            "T4": {
              "type": "string"
            },
            "TSH": {
              "type": "string"
            }
          },
          "required": [
            "T4",
            "TSH"
          ]
        }
      },
      "required": [
        "^[0-9]{10}$"
      ]
    },
    "FileMorbidityCondition": {
      "type": "object",
      "properties": {
        "^[0-9]{10}$": {
          "type": "string"
        }
      },
      "required": [
        "^[0-9]{10}$"
      ]
    },
    "DateOfBirth": {
      "type": "string"
    },
    "DieticianId": {
      "type": "integer"
    },
    "Vitals": {
      "type": "object",
      "properties": {
       "^[0-9]{10}$": {
          "type": "object",
          "properties": {
            "Weight": {
              "type": "number"
            },
            "Height": {
              "type": "number"
            },
            "Temperature": {
              "type": "number"
            },
            "SP": {
              "type": "integer"
            },
            "DP": {
              "type": "integer"
            }
          },
          "required": [
            "Weight",
            "Height",
            "Temperature",
            "SP",
            "DP"
          ]
        }
      },
      "required": [
        "^[0-9]{10}$"
      ]
    },
    "LastVisitDate": {
      "type": "string"
    }
  },
  "required": [
    "patientId",
    "FirstName",
    "LastName",
    "ContactNumber",
    "Email",
    "Allergy",
    "FoodPreference",
    "CuisineCategory",
    "FileMorbidity",
    "FileMorbidityCondition",
    "DateOfBirth",
    "DieticianId",
    "Vitals",
    "LastVisitDate"
  ]
}